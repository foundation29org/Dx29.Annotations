{
  "ConnectionStrings": {
    "BlobStorage": "<connection-string>"
  },

  "ServiceBus": {
    "ConnectionString": "<connection-string>",
    "QueueName": "<queue-name>"
  },

  "SignalR": {
    "ConnectionString": "<connection-string>",
    "HubName": "<hub-name>"
  },

  "AppInsights": {
    "Key": "<key>"
  },

  "Segmentation": {
    "Endpoint": "http://localhost:8080/api/v1/"
  },

  "CognitiveServices": {
    "Endpoint": "<cognitive-service-endpoint>",
    "Authorization": "<token>",
    "Region": "<region>"
  },

  "NCRAnnotation": {
    "Endpoint": "<ncr-endpoint>",
    "Authorization": "<token>",
    "IsEnabled": false
  },

  "TAHAnnotation": {
    "Endpoint": "<tah-endpoint>",
    "Path": "text/analytics/v3.1/entities/health/jobs",
    "Authorization": "<token>",
    "BlackList": "",
    "IsEnabled": true
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
